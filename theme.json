  "styles": {
    "blocks": {
      "core/button": {
        "spacing": {
          "padding": {
            "bottom": "var(--wp--preset--spacing--x-small)",
            "left": "var(--wp--preset--spacing--small)",
            "right": "var(--wp--preset--spacing--small)",
            "top": "var(--wp--preset--spacing--x-small)"
          }
        },
        "typography": {
          "fontWeight": "var(--wp--custom--font-weight--bold)"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--base)",
      "text": "var(--wp--preset--color--main)"
    },
    "elements": {
      "button": {
        ":focus": {
          "color": {
            "background": "transparent",
            "text": "var(--wp--preset--color--primary)"
          },
          "outline": {
            "color": "var(--wp--preset--color--secondary)",
            "offset": "2px",
            "style": "dotted",
            "width": "1px"
          }
        },
        ":hover": {
          "color": {
            "background": "transparent",
            "text": "var(--wp--preset--color--primary)"
          }
        },
        "border": {
          "color": "var(--wp--preset--color--primary)",
          "radius": "var(--wp--preset--spacing--xx-small)",
          "style": "solid",
          "width": "var(--wp--preset--spacing--xx-small)"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--custom--color--primary-contrast)"
        },
        "spacing": {
          "padding": {
            "bottom": "var(--wp--preset--spacing--small)",
            "left": "var(--wp--preset--spacing--medium)",
            "right": "var(--wp--preset--spacing--medium)",
            "top": "var(--wp--preset--spacing--small)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontStyle": "normal",
          "fontWeight": "var(--wp--custom--font-weight--normal)",
          "textTransform": "uppercase"
        }
      },
      "caption": {
        "color": {
          "text": "var(--wp--preset--color--gray-dark)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--body)",
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "textTransform": "uppercase"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--heading)",
          "fontWeight": "var(--wp--custom--font-weight--bold)",
          "lineHeight": "var(--wp--custom--line-height--normal)"
        }
      },
      "link": {
        ":hover": {
          "typography": {
            "textDecoration": "underline"
          }
        },
        "color": {
          "text": "var(--wp--preset--color--secondary)"
        },
        "typography": {
          "textDecoration": "none"
        }
      }
    },
    "spacing": {
      "blockGap": "var(--wp--preset--spacing--medium)",
      "margin": {
        "bottom": "0",
        "left": "0",
        "right": "0",
        "top": "0"
      },
      "padding": {
        "bottom": "0",
        "left": "var(--wp--preset--spacing--medium)",
        "right": "var(--wp--preset--spacing--medium)",
        "top": "0"
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--body)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "fontWeight": "var(--wp--custom--font-weight--normal)",
      "lineHeight": "var(--wp--custom--line-height--normal)"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    },
    {
      "area": "sidebar",
      "name": "sidebar",
      "title": "Sidebar"
    }
  ],
  "version": 2,
  "$schema": "https://schemas.wp.org/wp/6.4/theme.json"
